{
  "name": "makaira/connect",
  "description": "OXID Connector to Makaira",
  "type": "oxideshop-module",
  "version": "1.0.0",
  "extra": {
    "oxideshop": {
      "target-directory": "makaira/connect"
    }
  },
  "require": {
    "php": ">=5.6",
    "makaira/shared-libs": "^2018.11",
    "marm/yamm": ">=1.4.0",
    "doctrine/dbal": "^2.5"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.0",
    "pdepend/pdepend": "@stable",
    "phpmd/phpmd": "@stable",
    "squizlabs/php_codesniffer": "@stable"
  },
  "repositories": [
    {
      "type": "git",
      "url": "git@gitlab.marmalade.de:makaira/shared-libs.git"
    }
  ],
  "autoload": {
    "classmap": ["src/"],
    "psr-0": {
        "Makaira": ["src/", "tests/"]
    },
    "psr-4": {
      "Makaira\\Connect\\": "src/Makaira/Connect/"
    }
  }
}
